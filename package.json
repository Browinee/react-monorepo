{
  "name": "react-monorepo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [
    "monorepo",
    "turbo",
    "react"
  ],
  "author": "justin",
  "license": "MIT",
  "scripts": {
    "dev": "turbo dev",
    "dev:sb": "turbo dev --filter=@justinlin/storybook",
    "build": "turbo build --filter=!@justinlin/storybook",
    "lint": "pnpm lint:packages",
    "lint:packages": "eslint packages/**/*.{ts,tsx}",
    "lint:packages-fix": "eslint --fix ./packages/**/*.{ts,tsx}",
    "format:check": "prettier --check packages/**/**/src --cache",
    "format:write": "prettier --write packages/**/**/src --cache",
    "test": "jest --verbose",
    "prepare": "husky install",
    "changeset": "changeset add",
    "version": "changeset version",
    "version:snapshot": "changeset version --snapshot dev",
    "release": "changeset publish",
    "release:snapshot": "changeset publish --snapshot --no-git-tag --tag dev"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@storybook/react": "^7.6.20",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/eslint": "^8.56.12",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "commitlint-plugin-function-rules": "^3.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "whatwg-fetch": "^3.6.20"
  },
  "dependencies": {
    "turbo": "^2.1.2"
  },
  "packageManager": "pnpm@8.15.5",
  "publishConfig": {
    "access": "public"
  }
}
